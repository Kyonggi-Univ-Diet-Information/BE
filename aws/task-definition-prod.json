{
  "requiresCompatibilities": ["FARGATE"],
  "family": "Kiryong-task",
  "containerDefinitions": [
    {
      "name": "kiryong-diet",
      "image": "920373025050.dkr.ecr.us-east-1.amazonaws.com/kiryong-diet:#APP_VERSION#",
      "portMappings": [
        {
          "name": "kiryong-task-port",
          "containerPort": 8081,
          "hostPort": 8081,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "ecs/kiryong-diet-logs",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::920373025050:role/kiryong-ecs-task-excution-role"
}

{
  "requiresCompatibilities": ["FARGATE"],
  "family": "Kiryong-diet-TaskDefinition-Prod",
  "containerDefinitions": [
    {
      "name": "kiryong-diet",
      "image": "920373025050.dkr.ecr.us-east-1.amazonaws.com/kiryong-diet:#APP_VERSION#",
      "portMappings": [
        {
          "name": "kiryong-port",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "secrets": [
        {
          "valueFrom": "arn:aws:secretsmanager:us-east-1:920373025050:secret:kiryong-secret-077V4f",
          "name": "SECRET_ENV"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/kiryong-diet-logs",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::920373025050:role/ecsTaskExecutionRole"
}

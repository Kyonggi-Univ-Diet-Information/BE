{
  "requiresCompatibilities": ["FARGATE"],
  "family": "Kiryong-diet-TaskDefinition-Prod",
  "containerDefinitions": [
    {
      "name": "kiryong-diet",
      "image": "920373025050.dkr.ecr.us-east-1.amazonaws.com/kiryong-diet:#APP_VERSION#",
      "portMappings": [
        {
          "name": "kiryong-port",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "secrets": [
        {
          "name": "DB_URL_KIRYONG",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:920373025050:secret:DB_URL_KIRYONG-dSpNuP"
        },
        {
          "name": "DB_USERNAME_KIRYONG",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:920373025050:secret:DB_USERNAME_KIRYONG-0L2KJX"
        },
        {
          "name": "DB_PASSWORD_KIRYONG",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:920373025050:secret:DB_PASSWORD_KIRYONG-hryKYM"
        }
      ]
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::920373025050:role/ecsTaskExecutionRole"
}
